.panel-heading {
	background: #75CF74;/* #f6f6f6; */
	border-radius: 5px 5px 0 0;
	border-bottom: 1px solid #DADADA;
	 /* padding: 18px; change */ 
	 padding: 10px;
	
	position: relative;
}


.panel-body {
	background: #99CC99;
	-webkit-box-shadow: 0 1px 1px rgba(0, 0, 0, 0.05);
	box-shadow: 0 1px 1px rgba(0, 0, 0, 0.05);
	border-radius: 5px;
	padding-top:10px;
}


.panel-footer {
	background-color: #75CF74;/* #f5f5f5; */
	border-radius: 0 0 5px 5px;
	margin-top: -5px;
}

.table > thead > tr{
	background-color: #75CF74;
}